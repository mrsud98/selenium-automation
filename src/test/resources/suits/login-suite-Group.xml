<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="tests" thread-count="2">

    <listeners>
        <listener class-name="com.iasys.utilities.ExtentReporter"/>
        <listener class-name="com.iasys.listeners.RetryTransformer"/>
    </listeners>

    <test name="smoke">
     <parameter name="browser" value="chrome"/>
           <groups>
               <run>
			      <include name="smoke"/>
			     </run>
			  </groups>
			   
	 <classes>
            <class name="com.iasys.tests.LoginTest"/>
       </classes>
    </test>
    
